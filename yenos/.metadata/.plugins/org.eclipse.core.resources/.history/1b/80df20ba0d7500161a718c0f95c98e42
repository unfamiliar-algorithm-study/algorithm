package linkedlist;

/**
 * @author yenos
 * @date 2016. 9. 4.
 */
public class main {

	public static void main(String[] args) {
		
		LinkedList l = new LinkedList();
		l.addLast("10");
		
		l.addLast("11");
		l.addLast("12");
		
		l.addFirst("9");
		l.showAllNode();
		
		//lsearchWithIndex Test
		
		int index = 0;
		System.out.println("index "+index+" => "+l.searchWithIndex(index)+"데이터를 가집니다.");
		int data = 9;
		System.out.println("data "+data+" => "+l.searchWithData("10")+"번쨰에 있어요.");
		
		l.middleAdd(0,"999");
	}
}
