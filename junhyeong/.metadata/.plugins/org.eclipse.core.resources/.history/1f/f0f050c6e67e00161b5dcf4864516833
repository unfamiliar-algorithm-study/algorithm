package highsort;


	
	public int partition(int arr[], int begin, int end){
		int pivot = arr[(begin+right)/2];
		
		while(begin < end){while ((arr[left] < pivot) && (left < right))
				left++;
			while ((arr[right] > pivot) && (left < right))
				right--;

			if (left < right) {
				int temp = arr[left];
				arr[left] = arr[right];
				arr[right] = temp;
			}
		}

		
	}
}
