public class main {

	public static void main(String[] args) {
		
		LinkedList l = new LinkedList();
		l.addLast("10");
		
		l.addLast("11");
		l.addLast("12");
		
		l.addFirst("9");
		l.showAllNode();
		
		//lsearchWithIndex Test
		
		int index = 0;
		System.out.println("index "+index+" => "+l.searchWithIndex(index));
		System.out.println("index "+index+" => "+l.searchWithData("9"));
		
//		System.out.println(l.searchWithIndex(1));
//		System.out.println(l.searchWithIndex(0));
		
		

		
