package normalsort;

public class SelectionSort {
	
	int[] m_arr;
	int temp;
	
	public SelectionSort(int[] arr){
		m_arr = arr;
		System.out.println("Solted arr = ");
		for(int min = 0; min < m_arr.length ; min++){
			for(int j = min+1 ; j < m_arr.length; j++){
				if(m_arr[min] > m_arr[j]){
					temp = m_arr[min];
					m_arr[min] = m_arr[j];
					m_arr[j] = temp;
				}
				System.out.print(m_arr[j]+" ");
			}
			System.out.println(" ");
			System.out.print(m_arr[min]+" ");
		}
		System.out.println(" ");
	}
}
