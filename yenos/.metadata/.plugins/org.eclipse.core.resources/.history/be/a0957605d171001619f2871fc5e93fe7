public class SpareMatrix {
	private int colum;
	private int row;
	private int value;
	
	private int[][] matrix;
	
	public SpareMatrix(){
		
	}
	
	public void createSM(int m, int n){
		for(int i=0;i<m;i++){
			for(int j=0;j<n;j++){
				this.matrix[i][j] = 0;
			}
		}
//		this.matrix = 
	}
