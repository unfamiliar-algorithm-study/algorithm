/**
 * 
 */
package queue;

/**
 * @author yenos
 * @date 2016. 9. 14.
 */
public class Deck {
	
	class Node{
		Node left;
		Node right;
		String data;
	}	
	
	Node front;
	Node rear;
	
	public void createDQ(){
		front = null;
		rear = null;		
	}
	
	
	//만약 큐가 비었다면 둘다 널일 경우이다. 
	public Boolean isEmptyDQ(){
		if (front == null && rear==null){
			return true;
		}else{
			return false;
		}
	}
	
	
	

}
