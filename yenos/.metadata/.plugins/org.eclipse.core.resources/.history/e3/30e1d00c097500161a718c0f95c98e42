 */
package linkedlist;

import javax.xml.stream.events.NotationDeclaration;

/**
 * @author yenos
 * @date 2016. 9. 4.
 */
public class LinkedList {
	
	class Node{
		String data;
		Node next;
	}

	private Node head = null;
	private Node lastNode = new Node();
	
	public LinkedList(){
		
	}
	
	public void addNode(String data){
		Node tmp = new Node();
		tmp.data = data;
		
		if(head==null){
			System.out.println("is First "+tmp.data);
			head =  new Node();
			//해드의 노등가 이노드를 가지고있는다.
			head.next = tmp;
			
			//라스트는 tmp자체가 되고 
			lastNode = tmp;
//			lastNode.next = null;
			
		}else{
			//현재의 라스트는?
			System.out.println("current add =>"+lastNode.data);
			
			//혀재라스트의 다음을 tmp로 넣어주
			lastNode.next = tmp;

			System.out.println("current add =>"+lastNode.next.data);
			//라스트는 다시 tmp로 설정한다.
//			lastNode = tmp;
			
//			lastNode.data = tmp.data;
//			lastNode.next = tmp;
//			System.out.println("last add =>"+tmp.data);
		}
//		System.out.println("last node=>"+lastNode.data);
	}
	public void searchNode(){
		
		Node tmp = new Node();
		tmp = head;
//		System.out.println(head.data);
		int i=0;
		while(tmp!=lastNode){		
			i++;				
			//새로운노
			Node a = new Node();
			//이노드는 탬프의 다음이고
			if(tmp!=null){
								
				a =tmp.next;
//				System.out.println(a.data);
//				System.out.println(tmp.next.next.data);
				tmp = a.next;				
				
			}else{
				
				break;
			}			
		}		
	}

}
