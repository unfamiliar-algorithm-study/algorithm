package sta;

/**
 * @author yenos
 * @date 2016. 9. 4.
 */
public class main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {

		Stack stack= new Stack();
		
		System.out.println("===== arrSTack =====");
		//ARR!!!!!
		stack.arrPush("hi");
		stack.arrPush("nice");
		stack.arrPush("meet");
		stack.arrShowAllStack();
//		//pop
//		stack.arrPop();
//		//show all
//		stack.arrShowAllStack();
//		//pop
//		stack.arrPop();
//		stack.arrShowAllStack();
		
		System.out.println("===== END=====");
		System.out.println();
		System.out.println("===== LinkStack =====");
		
		stack.linkPush("hi");
		stack.linkPush("nice");
		stack.linkPush("to");
		System.out.println("===== showall =====");

		stack.linkShowAllNode();
		System.out.println("===== pop"
				+ " =====");

		stack.linkPop();
		stack.linkPop();

		stack.linkPop();
//		stack.linkPop();

		stack.linkShowAllNode();
		System.out.println("===== END=====");
		
		
		String sick = "((A+B)*C)";
		
		String strFirst = sick.substring(2,3);
		System.out.println(strFirst);
		
		
		//연산자가 나오면 넣고.
		if(sick.equals("+") ||sick.equals("-")||sick.equals("*")||sick.equals("-") ){
			stack.linkPush(sick);
		}
		
		

		

		//영어나오면 그대로넘기고
		//A
		//연산자나오면 스택  +
		//AB
		//괄호닫는거나오면 pop 
		//AB+
		//스택 *
		//영어C
		//
		
		
		
		

		

	}

}
