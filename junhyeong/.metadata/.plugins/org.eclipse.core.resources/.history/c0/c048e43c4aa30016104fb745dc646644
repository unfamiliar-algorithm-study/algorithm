package dynamic_dfs;

public class Main {
	static final int h = 5, w = 4;
	static int[][] dp = new int[h+1][w+1];
	
	public static void main(String[] main){
		calc();
		System.out.println(dp[0][0]);
		System.out.println(dp[1][1]);
		System.out.println(dp[5][4]);
	}
	
	public static void calc(){
		int i, j;
		dp[0][0] = 1;
		for(i = 0; i<=h; i++){
			for(j = 0; j<= w; j++){
				if(i != 0)
					dp[i][j] += dp[i-1][j];
				if(j != 0)
					dp[i][j] += dp[i][j-1];
			}
		}
	}
}
